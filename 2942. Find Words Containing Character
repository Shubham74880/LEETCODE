class Solution {
public:
    vector<int> findWordsContaining(vector<string>& words, char x) {
        int s = words.size();
     vector<int> ans;
        for(int i =0;i<s;i++){
            if(words[i].find(x)!=string::npos){
                ans.push_back(i);
            }
        }
        return ans;
    }
};
