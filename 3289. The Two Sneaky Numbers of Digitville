class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        vector<int> ans;
        unordered_map<int,int> freq;

        for(int x : nums){
            freq[x]++;
        }
        for(auto [num,k] : freq){
            if(k==2){
                ans.push_back(num);
            }
        }
        
        return ans;
    }
};
